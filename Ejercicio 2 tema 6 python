class Alumno:

    def inicializar(self, nombre, nota):
        self.nombre=nombre
        self.nota=nota

    def imprimir(self):
        print("Nombre", self.nombre)
        print("nota", self.nota)

    def mostrarEstado(self):

        if self.nota >=5:
            print("Aprobado")
        else:
            print("Suspenso")

alumno1=Alumno()
alumno1.inicializar("Rafa", 10)
alumno1.imprimir()
alumno1.mostrarEstado()

alumno2=Alumno()
alumno2.inicializar("Sixto", 2)
alumno2.imprimir()
alumno2.mostrarEstado()
